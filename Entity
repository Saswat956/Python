import spacy

def extract_entities(paragraph):
    nlp = spacy.load("en_core_web_sm")
    doc = nlp(paragraph)

    entities = []
    for entity in doc.ents:
        entities.append({
            'text': entity.text,
            'label': entity.label_
        })

    return entities

# Example usage:
paragraph = "Apple Inc. was founded by Steve Jobs and Steve Wozniak in 1976. It is headquartered in Cupertino, California."
entities = extract_entities(paragraph)
for entity in entities:
    print(entity['text'], entity['label'])
